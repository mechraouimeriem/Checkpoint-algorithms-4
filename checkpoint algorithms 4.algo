ALGORITHM Tri_Insertion
VAR
    tab : ARRAY_OF INTEGER[];
    i, j , temp: INTEGER;
BEGIN
    FOR i FROM 0 TO tab.length-1 DO
       Read(tab[i]);
    END_FOR

   FOR i FROM 1 TO tab.length-1 DO
       temp := tab[i];
       j := i -1;
       WHILE ((j > 0) and (tab[j] > temp)) DO
            tab[j + 1] :=  tab[j];              
            j := j - 1;
        END_WHILE
        tab[j + 1] := temp;
   END_FOR
END